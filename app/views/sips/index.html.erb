<h1>Listing sips</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th>Contact</th>
    <th>Lon</th>
    <th>Lat</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sips.each do |sip| %>
  <tr>
    <td><%= sip.name %></td>
    <td><%= sip.category %></td>
    <td><%= sip.contact %></td>
    <td><%= sip.lon %></td>
    <td><%= sip.lat %></td>
    <td><%= link_to 'Show', sip %></td>
    <td><%= link_to 'Edit', edit_sip_path(sip) %></td>
    <td><%= link_to 'Destroy', sip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sip', new_sip_path %>
